{
  "name": "node-notebook",
  "version": "0.3.9",
  "description": "A notebook service that runs Javascript through the node vm",
  "main": "./lib/vm.js",
  "bin": {
    "node-notebook": "bin/node-notebook.js"
  },
  "scripts": {
    "coverage": "istanbul cover _mocha test/vm.js;",
    "postinstall": "mv ./node_modules/font-awesome ./views/assets/font-awesome;npm run build;",
    "test": "npm run lint; mocha; npm run coverage;",
    "lint": "./node_modules/.bin/eslint .;",
    "build": "lessc views/index/index.less views/index/index.css --clean-css;lessc views/notebook/notebook.less views/notebook/notebook.css --clean-css;browserify views/notebook/notebook.js | uglifyjs > views/notebook/bundle.js",
    "start": "node index.js"
  },
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "chart.js": "^2.3.0",
    "codemirror": "^5.19.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "figlet": "^1.2.0",
    "filesize": "^3.3.0",
    "font-awesome": "^4.6.3",
    "js-beautify": "^1.6.4",
    "moment": "^2.15.1",
    "node-flat-db": "^0.2.1",
    "psychic-ui": "^0.4.0",
    "pug": "^2.0.0-alpha7"
  },
  "devDependencies": {
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "eslint": "^3.8.1",
    "grunt": "^1.0.1",
    "grunt-contrib-pug": "^1.0.0",
    "grunt-screenshot": "^0.6.0",
    "istanbul": "^0.4.5",
    "less": "^2.7.1",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.0.1",
    "supertest": "^2.0.0",
    "uglifyjs": "^2.4.10"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcsapo/node-notebook.git"
  },
  "bugs": {
    "url": "https://github.com/gabrielcsapo/node-notebook/issues"
  },
  "homepage": "https://github.com/gabrielcsapo/node-notebook#readme"
}
